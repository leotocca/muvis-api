{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node-server" }],
  "env": { "TMDB_API_KEY": "@tmdb_api_key" },
  "routes": [
    {
      "src": "/",
      "dest": "index.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/genres",
      "dest": "index.js",
      "methods": ["GET"]
    },
    {
      "src": "/years",
      "dest": "index.js",
      "methods": ["GET"]
    },
    {
      "src": "/rates",
      "dest": "index.js",
      "methods": ["GET"]
    }
  ]
}
