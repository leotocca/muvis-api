{
  "version": 2,
  "builds": [{ "src": "app.js", "use": "@now/node-server" }],
  "routes": [
    {
      "src": "/",
      "dest": "/app.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/genres",
      "dest": "/app.js",
      "methods": ["GET"]
    },
    {
      "src": "/years",
      "dest": "/app.js",
      "methods": ["GET"]
    },
    {
      "src": "/rates",
      "dest": "/app.js",
      "methods": ["GET"]
    }
  ]
}
